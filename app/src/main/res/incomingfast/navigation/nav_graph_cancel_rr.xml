<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph_cancel_rr"
    app:startDestination="@id/recebaRapidoCancellationReasonFragment">

    <fragment
        android:id="@+id/cancelattionRecebaRapidoComparationFragment"
        android:name="br.com.mobicare.cielo.recebaRapido.cancellation.comparationscreen.CancelattionRecebaRapidoComparationFragment"
        android:label="fragment_cancelattion_receba_rapido_comparation"
        tools:layout="@layout/fragment_cancelattion_receba_rapido_comparation" >
        <action
            android:id="@+id/action_cancelattionRecebaRapidoComparationFragment_to_recebaRapidoCancellationReasonFragment"
            app:destination="@id/recebaRapidoCancellationReasonFragment"
            app:enterAnim="@anim/slide_from_right"
            app:exitAnim="@anim/slide_to_left"
            app:popEnterAnim="@anim/slide_from_left"
            app:popExitAnim="@anim/slide_to_right"/>
        <argument
            android:name="PLAN"
            app:argType="string"
            app:nullable="true" />

    </fragment>
    <fragment
        android:id="@+id/recebaRapidoCancellationReasonFragment"
        android:name="br.com.mobicare.cielo.recebaRapido.cancellation.selectreason.RecebaRapidoCancellationReasonFragment"
        android:label="fragment_receba_rapido_cancellation_reason"
        tools:layout="@layout/fragment_receba_rapido_cancellation_reason" >
        <action
            android:id="@+id/action_recebaRapidoCancellationReasonFragment_to_cancellationRequestedFragment"
            app:destination="@id/cancellationRequestedFragment"
            app:enterAnim="@anim/slide_from_right"
            app:exitAnim="@anim/slide_to_left"
            app:popEnterAnim="@anim/slide_from_left"
            app:popExitAnim="@anim/slide_to_right"/>
        <argument
            android:name="PLAN"
            app:argType="string"
            app:nullable="true" />
        <argument
            android:name="EXTRA_TAX_COMPARATION"
            app:argType="br.com.mobicare.cielo.taxaPlanos.mapper.ComparationViewModelRR"
            app:nullable="true" />
    </fragment>
    <fragment
        android:id="@+id/cancellationRequestedFragment"
        android:name="br.com.mobicare.cielo.recebaRapido.cancellation.requested.CancellationRequestedFragment"
        android:label="fragment_cancellation_requested"
        tools:layout="@layout/fragment_cancellation_requested" >
        <argument
            android:name="PLAN"
            app:argType="string"
            app:nullable="true" />
        <argument
            android:name="EXTRA_TAX_COMPARATION"
            app:argType="br.com.mobicare.cielo.taxaPlanos.mapper.ComparationViewModelRR"
            app:nullable="true" />
    </fragment>
</navigation>