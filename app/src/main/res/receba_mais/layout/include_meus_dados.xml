<?xml version="1.0" encoding="utf-8"?>

<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">


    <LinearLayout
        android:id="@+id/linearLayout4"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:layout_marginLeft="16dp"
        android:layout_marginTop="33dp"
        android:layout_marginEnd="8dp"
        android:layout_marginRight="8dp"
        android:layout_marginBottom="8dp"
        android:gravity="center_vertical"
        android:orientation="horizontal"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0"
        tools:ignore="MissingConstraints">

        <ImageView
            android:layout_width="24dp"
            android:layout_height="24dp"
            android:layout_marginRight="8dp"
            app:srcCompat="@drawable/ic_menu_contato" />

        <br.com.mobicare.cielo.commons.presentation.utils.custom.TypefaceTextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Meus dados"
            style="@style/TextBlack_263238_16sp_700"
            android:textColor="@color/text_color_263238"
            android:textSize="16sp"
            app:typeface="MuseoSans-700.ttf" />

    </LinearLayout>


    <br.com.mobicare.cielo.commons.presentation.utils.custom.TypefaceTextView
        android:id="@+id/typefaceTextView18"
        android:layout_width="@dimen/dimen_0dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        android:layout_marginEnd="16dp"
        android:layout_marginRight="16dp"
        android:layout_marginBottom="8dp"
        android:text="Antes de confirmar sua contratação, verifique se seus dados estão corretos. O contrato será enviado para o seu e-mail"
        android:textColor="@color/purple"
        android:textSize="12sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/linearLayout4"
        app:layout_constraintTop_toBottomOf="@+id/linearLayout4"
        app:layout_constraintVertical_bias="0.0"
        app:typeface="MuseoSans-500.ttf"
        tools:ignore="MissingConstraints" />

    <br.com.mobicare.cielo.commons.presentation.utils.custom.TypefaceTextView
        android:id="@+id/typefaceTextView29"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        android:layout_marginEnd="8dp"
        android:layout_marginRight="8dp"
        android:layout_marginBottom="8dp"
        android:text="Telefone:"
        android:textColor="@color/grey_707070"
        android:textSize="14sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/typefaceTextView18"
        app:layout_constraintTop_toBottomOf="@+id/typefaceTextView18"
        app:layout_constraintVertical_bias="0.0"
        app:typeface="MuseoSans-500.ttf"
        tools:ignore="MissingConstraints" />

    <br.com.mobicare.cielo.commons.presentation.utils.custom.TypefaceTextView
        android:id="@+id/sc_tv_phone"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="4dp"
        android:layout_marginEnd="8dp"
        android:layout_marginRight="8dp"
        android:layout_marginBottom="8dp"
        android:text="(92) 99999-9999"
        android:textColor="@color/grey_707070"
        android:textSize="14sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/typefaceTextView29"
        app:layout_constraintTop_toBottomOf="@+id/typefaceTextView29"
        app:layout_constraintVertical_bias="0.0"
        app:typeface="MuseoSans-900"
        tools:ignore="MissingConstraints" />

    <br.com.mobicare.cielo.commons.presentation.utils.custom.TypefaceTextView
        android:id="@+id/typefaceTextView20"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        android:layout_marginEnd="8dp"
        android:layout_marginRight="8dp"
        android:layout_marginBottom="8dp"
        android:text="E-mail:"
        android:textColor="@color/grey_707070"
        android:textSize="14sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/sc_tv_phone"
        app:layout_constraintTop_toBottomOf="@+id/sc_tv_phone"
        app:layout_constraintVertical_bias="0.0"
        app:typeface="MuseoSans-500.ttf"
        tools:ignore="MissingConstraints" />


    <br.com.mobicare.cielo.commons.presentation.utils.custom.TypefaceTextView
        android:id="@+id/sc_tv_email"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="4dp"
        android:layout_marginEnd="8dp"
        android:layout_marginRight="8dp"
        android:layout_marginBottom="8dp"
        android:text="joao@gmail.com"
        android:textColor="@color/grey_707070"
        android:textSize="14sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/typefaceTextView20"
        app:layout_constraintTop_toBottomOf="@+id/typefaceTextView20"
        app:layout_constraintVertical_bias="0.0"
        app:typeface="MuseoSans-900"
        tools:ignore="MissingConstraints" />


</androidx.constraintlayout.widget.ConstraintLayout>
