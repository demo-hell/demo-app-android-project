<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:padding="@dimen/dimen_16dp"
    android:layout_margin="@dimen/dimen_8dp"
    xmlns:tools="http://schemas.android.com/tools"
    android:background="@drawable/shape_rounded"
    android:id="@+id/dialog"
    android:orientation="vertical">

    <TextView
        android:id="@+id/custom_dialog_title"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        tools:text="Deseja sobrescrever o token?"
        style="@style/TextTitleStyleMontserrat16spBlack231F20"/>


    <TextView
        android:id="@+id/custom_dialog_message"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/dimen_8dp"
        tools:text="Já existe um token cadastrado neste dispositivo ao realizar uma nova solicitação será apagado o token atual."
        style="@style/TextBlack14spUbuntuRegular_231F20"/>


    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_marginTop="@dimen/dimen_16dp"
        android:orientation="horizontal"
        >

        <mehdi.sakout.fancybuttons.FancyButton
            android:id="@+id/custom_dialog_button_left"
            android:layout_width="@dimen/dimen_0dp"
            android:layout_weight="1"
            android:layout_marginEnd="@dimen/dimen_16dp"
            android:layout_height="wrap_content"
            style="@style/RoundedButtonWhiteBackWithBlueBorderFlui"/>


        <mehdi.sakout.fancybuttons.FancyButton
            android:id="@+id/custom_dialog_button_right"
            android:layout_width="@dimen/dimen_0dp"
            android:layout_weight="1"
            android:layout_height="wrap_content"
            style="@style/RoundedButtonWhiteBackWithBlueBorderFlui"
             />

    </LinearLayout>


</LinearLayout>