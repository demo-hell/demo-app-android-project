<?xml version="1.0" encoding="utf-8"?>
<androidx.core.widget.NestedScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/white"
    android:fillViewport="true"
    tools:context=".pix.ui.mylimits.home.PixMyLimitsFragment">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@color/white"
        android:orientation="vertical"
        android:padding="@dimen/dimen_16dp"
        android:paddingBottom="@dimen/dimen_10dp"
        app:layout_constraintTop_toTopOf="parent">

        <androidx.appcompat.widget.AppCompatTextView
            android:id="@+id/tv_my_limits_title"
            style="@style/medium_montserrat_18_cloud_500"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="@string/text_pix_my_limits_settings"
            app:layout_constraintTop_toTopOf="parent" />

        <androidx.appcompat.widget.AppCompatTextView
            android:id="@+id/tv_my_limits_subtitle"
            style="@style/regular_ubuntu_14_cloud_400"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/dimen_8dp"
            android:text="@string/text_pix_my_limits_manager"
            app:layout_constraintTop_toBottomOf="@+id/tv_my_limits_title" />

        <androidx.appcompat.widget.AppCompatTextView
            android:id="@+id/tv_my_limits_pix_pf"
            style="@style/regular_ubuntu_16_cloud_500"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/dimen_14dp"
            android:layout_marginEnd="@dimen/dimen_5dp"
            android:drawableEnd="@drawable/ic_arrow_flui_display_200"
            android:drawablePadding="@dimen/dimen_8dp"
            android:paddingVertical="@dimen/dimen_10dp"
            android:text="@string/text_toolbar_pix_my_limits_pix_pf"
            app:layout_constraintTop_toBottomOf="@+id/tv_my_limits_subtitle" />

        <View
            android:id="@+id/view_my_limits_pix_pf"
            android:layout_width="match_parent"
            android:layout_height="@dimen/dimen_1dp"
            android:layout_marginTop="@dimen/dimen_6dp"
            android:background="@color/display_200"
            app:layout_constraintTop_toBottomOf="@+id/tv_my_limits_pix_pf" />

        <androidx.appcompat.widget.AppCompatTextView
            android:id="@+id/tv_my_limits_pix_pj"
            style="@style/regular_ubuntu_16_cloud_500"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/dimen_6dp"
            android:layout_marginEnd="@dimen/dimen_5dp"
            android:drawableEnd="@drawable/ic_arrow_flui_display_200"
            android:drawablePadding="@dimen/dimen_8dp"
            android:paddingVertical="@dimen/dimen_10dp"
            android:text="@string/text_toolbar_pix_my_limits_pix_pj"
            app:layout_constraintTop_toBottomOf="@+id/view_my_limits_pix_pf" />

        <View
            android:id="@+id/view_my_limits_pix_pj"
            android:layout_width="match_parent"
            android:layout_height="@dimen/dimen_1dp"
            android:layout_marginTop="@dimen/dimen_6dp"
            android:background="@color/display_200"
            app:layout_constraintTop_toBottomOf="@+id/tv_my_limits_pix_pj" />

        <androidx.appcompat.widget.AppCompatTextView
            android:id="@+id/tv_my_limits_withdraw_and_change"
            style="@style/regular_ubuntu_16_cloud_500"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/dimen_6dp"
            android:layout_marginEnd="@dimen/dimen_5dp"
            android:drawableEnd="@drawable/ic_arrow_flui_display_200"
            android:drawablePadding="@dimen/dimen_8dp"
            android:paddingVertical="@dimen/dimen_10dp"
            android:text="@string/text_toolbar_pix_my_limits_withdraw_and_change"
            app:layout_constraintTop_toBottomOf="@+id/view_my_limits_pix_pj" />

        <View
            android:id="@+id/view_withdraw_and_change"
            android:layout_width="match_parent"
            android:layout_height="@dimen/dimen_1dp"
            android:layout_marginTop="@dimen/dimen_6dp"
            android:background="@color/display_200"
            app:layout_constraintTop_toBottomOf="@+id/tv_my_limits_withdraw_and_change" />

        <androidx.appcompat.widget.AppCompatTextView
            android:id="@+id/tv_my_limits_account_registration"
            style="@style/regular_ubuntu_16_cloud_500"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/dimen_6dp"
            android:layout_marginEnd="@dimen/dimen_5dp"
            android:drawableEnd="@drawable/ic_arrow_flui_display_200"
            android:drawablePadding="@dimen/dimen_8dp"
            android:paddingVertical="@dimen/dimen_10dp"
            android:text="@string/text_toolbar_pix_my_limits_account_registration"
            app:layout_constraintTop_toBottomOf="@+id/view_withdraw_and_change" />

        <View
            android:id="@+id/view_trusted_contacts"
            android:layout_width="match_parent"
            android:layout_height="@dimen/dimen_1dp"
            android:layout_marginTop="@dimen/dimen_6dp"
            android:background="@color/display_200"
            app:layout_constraintTop_toBottomOf="@+id/tv_my_limits_account_registration" />

        <androidx.appcompat.widget.AppCompatTextView
            android:id="@+id/tv_my_limits_time_management"
            style="@style/regular_ubuntu_16_cloud_500"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/dimen_6dp"
            android:layout_marginEnd="@dimen/dimen_5dp"
            android:drawableEnd="@drawable/ic_arrow_flui_display_200"
            android:drawablePadding="@dimen/dimen_8dp"
            android:paddingVertical="@dimen/dimen_10dp"
            android:text="@string/text_toolbar_pix_my_limits_time_management"
            app:layout_constraintTop_toBottomOf="@+id/view_trusted_contacts" />

        <View
            android:id="@+id/view_time_management"
            android:layout_width="match_parent"
            android:layout_height="@dimen/dimen_1dp"
            android:layout_marginTop="@dimen/dimen_6dp"
            android:background="@color/display_200"
            app:layout_constraintTop_toBottomOf="@+id/tv_my_limits_time_management" />

    </androidx.constraintlayout.widget.ConstraintLayout>

</androidx.core.widget.NestedScrollView>