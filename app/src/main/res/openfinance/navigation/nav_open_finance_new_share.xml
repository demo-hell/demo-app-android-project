<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_open_finance_new_share"
    app:startDestination="@id/openFinanceNewShareFragment">
    <fragment
        android:id="@+id/openFinanceNewShareFragment"
        android:name="br.com.mobicare.cielo.openFinance.presentation.manager.newShare.OpenFinanceNewShareFragment"
        android:label="OpenFinanceNewShareFragment"
        tools:layout="@layout/open_finance_new_share_fragment">
        <action
            android:id="@+id/action_openFinanceNewShareFragment_to_openFinanceRedirectFragment"
            app:destination="@id/openFinanceRedirectFragment"
            app:enterAnim="@anim/slide_from_right"
            app:exitAnim="@anim/slide_to_left"
            app:popEnterAnim="@anim/slide_from_left"
            app:popExitAnim="@anim/slide_to_right" />
    </fragment>
    <fragment
        android:id="@+id/openFinanceRedirectFragment"
        android:name="br.com.mobicare.cielo.openFinance.presentation.manager.newShare.redirect.OpenFinanceRedirectFragment"
        android:label="OpenFinanceRedirectFragment"
        tools:layout="@layout/open_finance_redirect_fragment">
        <argument
            android:name="@string/redirect_url"
            app:argType="string" />
    </fragment>

</navigation>