<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="dialog_animation">
        <item name="android:windowEnterAnimation">@anim/slide_from_bottom_to_up</item>
        <item name="android:windowExitAnimation">@anim/slide_from_up_to_bottom</item>
    </style>


    <style name="FluiTabLayoutStyle" parent="Widget.Design.TabLayout">
        <item name="tabIndicatorColor">@color/blue_017CEB</item>
        <item name="android:background">@color/white</item>
    </style>

    <style name="FluiTabTextStyle" parent="android:Widget.TextView">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:gravity">center_vertical</item>
        <item name="android:textSize">16sp</item>
        <item name="android:textColor">@color/blue_017CEB</item>
    </style>


</resources>