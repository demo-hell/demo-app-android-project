<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/constraint_main"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">

    <androidx.cardview.widget.CardView
        android:id="@+id/card_view"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_margin="@dimen/dimen_16dp"
        app:cardCornerRadius="@dimen/dimen_10dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical">

            <androidx.constraintlayout.widget.ConstraintLayout
                android:id="@+id/contrant_view_title_est"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_marginTop="@dimen/dimen_8dp">

                <br.com.mobicare.cielo.commons.presentation.utils.custom.TypefaceTextView
                    android:id="@+id/text_title"
                    style="@style/MCN_TextTitleContent"
                    android:layout_width="wrap_content"
                    android:layout_height="@dimen/dimen_16dp"
                    android:layout_marginHorizontal="@dimen/dimen_16dp"
                    android:ellipsize="end"
                    android:maxLines="1"
                    android:text="@string/mcn_text_title_section_establishment"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintEnd_toStartOf="@+id/imageView_arrow"
                    app:layout_constraintHorizontal_bias="0.0"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent" />

                <ImageView
                    android:id="@+id/imageView_arrow"
                    android:layout_width="@dimen/dimen_24dp"
                    android:layout_height="@dimen/dimen_24dp"
                    android:layout_alignParentEnd="true"
                    android:layout_centerInParent="true"
                    android:layout_marginVertical="@dimen/dimen_8dp"
                    android:layout_marginEnd="@dimen/dimen_16dp"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintTop_toTopOf="parent"
                    app:layout_constraintVertical_bias="0.0"
                    app:srcCompat="@drawable/ic_arrow" />
            </androidx.constraintlayout.widget.ConstraintLayout>

            <androidx.constraintlayout.widget.ConstraintLayout
                android:id="@+id/constraint_view_details_establishment"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/contrant_view_title_est">

                <View
                    android:id="@+id/view5"
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/dimen_1dp"
                    android:layout_marginHorizontal="@dimen/dimen_16dp"
                    android:layout_marginBottom="@dimen/dimen_8dp"
                    android:background="@color/withe_eaeaea"
                    app:layout_constraintTop_toTopOf="parent" />

                <androidx.appcompat.widget.AppCompatTextView
                    android:id="@+id/tvAddEc"
                    style="@style/Paragraph_300_Regular_14_Brand_400"
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/dimen_48dp"
                    android:layout_marginTop="@dimen/dimen_8dp"
                    android:clickable="true"
                    android:drawableStart="@drawable/plus_circle"
                    android:drawablePadding="@dimen/dimen_8dp"
                    android:focusable="true"
                    android:gravity="center_vertical"
                    android:paddingHorizontal="@dimen/dimen_16dp"
                    android:text="@string/change_ec_add_ec"
                    android:textColor="@color/brand_400"
                    android:visibility="gone"
                    app:layout_constraintTop_toBottomOf="@id/view5"
                    tools:visibility="visible" />

                <br.com.mobicare.cielo.commons.presentation.utils.custom.TypefaceTextView
                    android:id="@+id/text_link0"
                    style="@style/Text12sp500Black263238"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/dimen_16dp"
                    android:layout_marginTop="@dimen/dimen_16dp"
                    android:text="@string/mcn_text_field_trading_name"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/tvAddEc" />

                <LinearLayout
                    android:id="@+id/editButton"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/dimen_32dp"
                    android:background="?attr/selectableItemBackground"
                    android:clickable="true"
                    android:focusable="true"
                    android:gravity="center_vertical"
                    android:orientation="horizontal"
                    app:layout_constraintBottom_toBottomOf="@+id/text_link0"
                    app:layout_constraintStart_toEndOf="@+id/text_link0"
                    app:layout_constraintTop_toTopOf="@+id/text_link0">

                    <androidx.appcompat.widget.AppCompatImageView
                        android:layout_width="@dimen/dimen_15dp"
                        android:layout_height="@dimen/dimen_15dp"
                        app:srcCompat="@drawable/ic_edit" />

                    <br.com.mobicare.cielo.commons.presentation.utils.custom.TypefaceTextView
                        style="@style/Text14sp_500Regular_00aeef"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="@dimen/dimen_6dp"
                        android:text="@string/text_edit_label" />
                </LinearLayout>

                <br.com.mobicare.cielo.commons.presentation.utils.custom.TypefaceTextView
                    android:id="@+id/text_fantasy_name"
                    style="@style/TextGrey12sp700"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginHorizontal="@dimen/dimen_16dp"
                    android:layout_marginTop="@dimen/dimen_8dp"
                    android:ellipsize="end"
                    android:maxLines="1"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/text_link0" />

                <br.com.mobicare.cielo.commons.presentation.utils.custom.TypefaceTextView
                    android:id="@+id/text_link1"
                    style="@style/Text12sp500Black263238"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginHorizontal="@dimen/dimen_16dp"
                    android:layout_marginTop="@dimen/dimen_16dp"
                    android:text="@string/mcn_text_field_number_establishment"
                    app:layout_constraintTop_toBottomOf="@+id/text_fantasy_name" />

                <br.com.mobicare.cielo.commons.presentation.utils.custom.TypefaceTextView
                    android:id="@+id/text_number_stabliment"
                    style="@style/TextGrey12sp700"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginHorizontal="@dimen/dimen_16dp"
                    android:layout_marginTop="@dimen/dimen_8dp"
                    app:layout_constraintTop_toBottomOf="@+id/text_link1" />

                <br.com.mobicare.cielo.commons.presentation.utils.custom.TypefaceTextView
                    android:id="@+id/text_link11"
                    style="@style/Text12sp500Black263238"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginHorizontal="@dimen/dimen_16dp"
                    android:layout_marginTop="@dimen/dimen_16dp"
                    android:text="@string/mcn_text_field_status_register"
                    app:layout_constraintTop_toBottomOf="@+id/text_number_stabliment" />

                <br.com.mobicare.cielo.commons.presentation.utils.custom.TypefaceTextView
                    android:id="@+id/text_ative"
                    style="@style/TextGrey12sp700"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginHorizontal="@dimen/dimen_16dp"
                    android:layout_marginTop="@dimen/dimen_8dp"
                    app:layout_constraintTop_toBottomOf="@+id/text_link11" />

                <br.com.mobicare.cielo.commons.presentation.utils.custom.TypefaceTextView
                    android:id="@+id/text_link31"
                    style="@style/Text12sp500Black263238"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginHorizontal="@dimen/dimen_16dp"
                    android:layout_marginTop="@dimen/dimen_16dp"
                    android:text="@string/mcn_text_field_block_debit_payment"
                    app:layout_constraintTop_toBottomOf="@+id/text_ative" />

                <br.com.mobicare.cielo.commons.presentation.utils.custom.TypefaceTextView
                    android:id="@+id/text_payment_block_debit"
                    style="@style/TextGrey12sp700"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginHorizontal="@dimen/dimen_16dp"
                    android:layout_marginTop="@dimen/dimen_8dp"
                    app:layout_constraintTop_toBottomOf="@+id/text_link31" />

                <br.com.mobicare.cielo.commons.presentation.utils.custom.TypefaceTextView
                    android:id="@+id/text_link41"
                    style="@style/Text12sp500Black263238"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginHorizontal="@dimen/dimen_16dp"
                    android:layout_marginTop="@dimen/dimen_16dp"
                    android:text="@string/mcn_text_field_date_open"
                    app:layout_constraintTop_toBottomOf="@+id/text_payment_block_debit" />

                <br.com.mobicare.cielo.commons.presentation.utils.custom.TypefaceTextView
                    android:id="@+id/text_date_open"
                    style="@style/TextGrey12sp700"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginHorizontal="@dimen/dimen_16dp"
                    android:layout_marginTop="@dimen/dimen_8dp"
                    app:layout_constraintTop_toBottomOf="@+id/text_link41" />

                <br.com.mobicare.cielo.commons.presentation.utils.custom.TypefaceTextView
                    android:id="@+id/text_link51"
                    style="@style/Text12sp500Black263238"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginHorizontal="@dimen/dimen_16dp"
                    android:layout_marginTop="@dimen/dimen_16dp"
                    android:text="@string/mcn_text_field_company_name"
                    app:layout_constraintTop_toBottomOf="@+id/text_date_open" />

                <br.com.mobicare.cielo.commons.presentation.utils.custom.TypefaceTextView
                    android:id="@+id/text_main_name"
                    style="@style/TextGrey12sp700"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginHorizontal="@dimen/dimen_16dp"
                    android:layout_marginTop="@dimen/dimen_8dp"
                    app:layout_constraintTop_toBottomOf="@+id/text_link51" />

                <br.com.mobicare.cielo.commons.presentation.utils.custom.TypefaceTextView
                    android:id="@+id/text_link61"
                    style="@style/Text12sp500Black263238"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginHorizontal="@dimen/dimen_16dp"
                    android:layout_marginTop="@dimen/dimen_16dp"
                    android:text="@string/mcn_text_field_cnpj"
                    app:layout_constraintTop_toBottomOf="@+id/text_main_name" />

                <br.com.mobicare.cielo.commons.presentation.utils.custom.TypefaceTextView
                    android:id="@+id/text_cnpj"
                    style="@style/TextGrey12sp700"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginHorizontal="@dimen/dimen_16dp"
                    android:layout_marginTop="@dimen/dimen_8dp"
                    app:layout_constraintTop_toBottomOf="@+id/text_link61" />

                <br.com.mobicare.cielo.commons.presentation.utils.custom.TypefaceTextView
                    android:id="@+id/text_link71"
                    style="@style/Text12sp500Black263238"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginHorizontal="@dimen/dimen_16dp"
                    android:layout_marginTop="@dimen/dimen_16dp"
                    android:text="@string/mcn_text_field_branch_activity"
                    app:layout_constraintTop_toBottomOf="@+id/text_cnpj" />

                <br.com.mobicare.cielo.commons.presentation.utils.custom.TypefaceTextView
                    android:id="@+id/text_mcc"
                    style="@style/TextGrey12sp700"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginHorizontal="@dimen/dimen_16dp"
                    android:layout_marginTop="@dimen/dimen_8dp"
                    app:layout_constraintTop_toBottomOf="@+id/text_link71" />

                <br.com.mobicare.cielo.commons.presentation.utils.custom.TypefaceTextView
                    android:id="@+id/text_link81"
                    style="@style/Text12sp500Black263238"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginHorizontal="@dimen/dimen_16dp"
                    android:layout_marginTop="@dimen/dimen_16dp"
                    android:text="@string/mcn_text_field_last_date_sale"
                    app:layout_constraintTop_toBottomOf="@+id/text_mcc" />

                <br.com.mobicare.cielo.commons.presentation.utils.custom.TypefaceTextView
                    android:id="@+id/text_date_last_sele"
                    style="@style/TextGrey12sp700"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginHorizontal="@dimen/dimen_16dp"
                    android:layout_marginTop="@dimen/dimen_8dp"
                    android:layout_marginBottom="@dimen/dimen_16dp"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/text_link81"
                    app:layout_constraintVertical_bias="0" />

                <br.com.mobicare.cielo.commons.presentation.utils.custom.TypefaceTextView
                    android:id="@+id/text_link91"
                    style="@style/Text12sp500Black263238"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginHorizontal="@dimen/dimen_16dp"
                    android:layout_marginTop="@dimen/dimen_16dp"
                    android:text="@string/mcn_text_field_branch_activity"
                    android:visibility="gone"
                    app:layout_constraintTop_toBottomOf="@+id/text_date_last_sele"
                    tools:visibility="visible" />

                <br.com.mobicare.cielo.commons.presentation.utils.custom.TypefaceTextView
                    android:id="@+id/text_branch"
                    style="@style/TextGrey12sp700"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginHorizontal="@dimen/dimen_16dp"
                    android:layout_marginTop="@dimen/dimen_8dp"
                    android:layout_marginBottom="@dimen/dimen_16dp"
                    android:visibility="gone"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/text_link91"
                    app:layout_constraintVertical_bias="0.0"
                    tools:visibility="visible" />
            </androidx.constraintlayout.widget.ConstraintLayout>
        </LinearLayout>
    </androidx.cardview.widget.CardView>
</androidx.constraintlayout.widget.ConstraintLayout>