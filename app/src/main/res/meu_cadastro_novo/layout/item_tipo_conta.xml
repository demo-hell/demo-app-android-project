<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/ll_descriptions"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    app:layout_constraintStart_toStartOf="parent"
    app:layout_constraintEnd_toEndOf="parent"
    app:layout_constraintTop_toBottomOf="@+id/dc_tv_name_bank">


    <br.com.mobicare.cielo.commons.presentation.utils.custom.TypefaceTextView
        android:id="@+id/dc_tv_num_agencia"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_weight="1"
        android:text="@string/item_dc_agencia"
        android:textColor="@color/title_black"
        android:textSize="12sp"
        app:typeface="MuseoSans-500.ttf"
        tools:ignore="MissingConstraints"
        android:layout_marginStart="@dimen/dimen_8dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"/>

    <br.com.mobicare.cielo.commons.presentation.utils.custom.TypefaceTextView
        android:id="@+id/dc_tv_num_agencia_impl"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_weight="1"
        android:textColor="@color/grey_838383"
        android:textSize="12sp"
        app:typeface="MuseoSans-500.ttf"
        tools:ignore="MissingConstraints"
        tools:text="0001"
        android:layout_marginStart="@dimen/dimen_8dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/dc_tv_num_agencia"/>




    <br.com.mobicare.cielo.commons.presentation.utils.custom.TypefaceTextView
        android:id="@+id/dc_tv_num_conta"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_weight="1"
        android:text="@string/item_dc_contas"
        android:textColor="@color/title_black"
        android:textSize="12sp"
        app:typeface="MuseoSans-500.ttf"
        tools:ignore="MissingConstraints"
        android:layout_marginStart="@dimen/dimen_8dp"
        app:layout_constraintStart_toEndOf="@+id/dc_tv_num_agencia"
        app:layout_constraintTop_toTopOf="parent"/>


    <br.com.mobicare.cielo.commons.presentation.utils.custom.TypefaceTextView
        android:id="@+id/dc_tv_num_conta_impl"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_weight="1"
        android:textColor="@color/grey_838383"
        android:textSize="12sp"
        app:layout_constraintStart_toStartOf="@+id/dc_tv_num_conta"
        app:layout_constraintTop_toBottomOf="@+id/dc_tv_num_conta"
        app:typeface="MuseoSans-500.ttf"
        tools:ignore="MissingConstraints"
        tools:text="1234567890-12345" />

    <br.com.mobicare.cielo.commons.presentation.utils.custom.TypefaceTextView
        android:id="@+id/dc_tv_tipo_conta"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_weight="1"
        android:text="@string/item_dc_tipo_conta"
        android:textColor="@color/title_black"
        android:textSize="12sp"
        app:typeface="MuseoSans-500.ttf"
        tools:ignore="MissingConstraints"
        android:layout_marginEnd="@dimen/dimen_8dp"
        android:layout_marginStart="@dimen/dimen_8dp"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintEnd_toEndOf="parent"/>

    <br.com.mobicare.cielo.commons.presentation.utils.custom.TypefaceTextView
        android:id="@+id/dc_tv_tipo_conta_impl"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_weight="1"
        android:paddingBottom="10dp"
        android:textColor="@color/grey_838383"
        android:textSize="12sp"
        app:typeface="MuseoSans-500.ttf"
        tools:text="Tipo de conta"
        tools:ignore="MissingConstraints"
        android:layout_marginEnd="@dimen/dimen_8dp"
        app:layout_constraintTop_toBottomOf="@+id/dc_tv_tipo_conta"
        app:layout_constraintStart_toStartOf="@+id/dc_tv_tipo_conta"/>


</androidx.constraintlayout.widget.ConstraintLayout>

