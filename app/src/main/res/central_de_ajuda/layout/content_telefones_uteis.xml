<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical">

        <br.com.mobicare.cielo.commons.presentation.utils.custom.TypefaceTextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="16dp"
            android:layout_marginTop="16dp"
            android:text="@string/central_ajuda_telefones_uteis"
            android:textColor="@color/text_color_263238"
            android:textSize="18sp"
            android:textStyle="bold"
            app:typeface="Museo700-Regular.ttf" />


        <androidx.recyclerview.widget.RecyclerView
            android:id="@+id/recycler_view_ajuda_telefones_uteis"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_marginBottom="16dp"/>



    </LinearLayout>


</layout>
