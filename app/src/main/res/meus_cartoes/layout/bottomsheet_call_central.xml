<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/grey_99000000">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/ll_call_central"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@drawable/background_welcome_notification"
        tools:layout_editor_absoluteX="-16dp"
        tools:layout_editor_absoluteY="-35dp">

        <br.com.mobicare.cielo.commons.presentation.utils.custom.TypefaceTextView
            android:id="@+id/typefaceTextView31"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="20dp"
            android:layout_marginTop="10dp"
            android:gravity="left|center"
            android:text="Ligue para central de relacionamento"
            android:textColor="@color/grey_5d5d5d"
            android:textSize="16sp"
            android:textStyle="normal"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:typeface="Museo300-Regular.ttf"
            tools:ignore="MissingConstraints" />

        <ImageView
            android:id="@+id/img_close"
            android:layout_width="25dp"
            android:layout_height="25dp"
            android:layout_marginEnd="10dp"
            app:layout_constraintBottom_toBottomOf="@+id/typefaceTextView31"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="1.0"
            app:layout_constraintStart_toEndOf="@+id/typefaceTextView31"
            app:layout_constraintTop_toTopOf="@+id/typefaceTextView31"
            app:srcCompat="@drawable/ic_symbol_close_brand_400_24_dp" />


        <View
            android:id="@+id/view2"
            android:layout_width="match_parent"
            android:layout_height="0.5dp"
            android:layout_marginStart="1dp"
            android:layout_marginTop="10dp"
            android:background="#B8B8B8"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/typefaceTextView31"
            tools:ignore="MissingConstraints" />

        <br.com.mobicare.cielo.commons.presentation.utils.custom.TypefaceTextView
            android:id="@+id/txt_phone_capital"
            android:layout_width="match_parent"
            android:layout_height="40dp"
            android:layout_marginTop="10dp"
            android:layout_weight=".9"
            android:gravity="center"
            android:text="@string/txt_call_central_one"
            android:textColor="@color/blue"
            android:textSize="18sp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/typefaceTextView31"
            tools:ignore="MissingConstraints" />

        <View
            android:id="@+id/view3"
            android:layout_width="match_parent"
            android:layout_height="0.5dp"
            android:layout_marginStart="1dp"
            android:layout_marginTop="5dp"
            android:background="#B8B8B8"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/txt_phone_capital"
            tools:ignore="MissingConstraints" />

        <br.com.mobicare.cielo.commons.presentation.utils.custom.TypefaceTextView
            android:id="@+id/txt_phone_local"
            android:layout_width="match_parent"
            android:layout_height="40dp"
            android:layout_marginStart="1dp"
            android:layout_marginTop="5dp"
            android:layout_weight=".9"
            android:gravity="center"
            android:text="@string/txt_call_central_two"
            android:textColor="@color/blue"
            android:textSize="18sp"
            android:layout_marginBottom="20dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/txt_phone_capital"
            tools:ignore="MissingConstraints" />

    </androidx.constraintlayout.widget.ConstraintLayout>

</androidx.constraintlayout.widget.ConstraintLayout>