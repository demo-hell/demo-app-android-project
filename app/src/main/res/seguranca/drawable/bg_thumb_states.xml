<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_checked="false">
        <shape android:shape="rectangle">
            <corners android:radius="@dimen/dimen_100dp" />
            <solid android:color="@color/color_ffffff_security" />
            <stroke android:color="@color/color_8c8c8c_security" android:width="@dimen/dimen_1dp"/>
            <size android:height="@dimen/dimen_20dp" />
        </shape>
    </item>
    <item android:state_checked="true">
        <shape android:shape="rectangle">
            <corners android:radius="@dimen/dimen_100dp" />
            <solid android:color="@color/color_ffffff_security" />
            <stroke android:color="@color/color_34c759_security" android:width="@dimen/dimen_1dp"/>
            <size android:height="@dimen/dimen_20dp" />
        </shape>
    </item>
</selector>