<?xml version="1.0" encoding="utf-8"?>
<selector
    xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_checked="false">
        <shape android:shape="oval">
            <solid android:color="@color/color_5A646E"/>
            <size android:width="@dimen/dimen_18dp"
                android:height="@dimen/dimen_18dp"/>
            <stroke android:width="@dimen/dimen_1dp"
                android:color="@color/color_5A646E_security"/>
        </shape>
    </item>
    <item android:state_checked="true">
        <shape android:shape="oval">
            <solid android:color="@color/color_34c759_security"/>
            <size android:width="@dimen/dimen_18dp"
                android:height="@dimen/dimen_18dp"/>
            <stroke android:width="@dimen/dimen_1dp"
                android:color="@color/color_34c759_security"/>
        </shape>
    </item>
</selector>