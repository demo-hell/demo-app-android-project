<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:layout_marginBottom="@dimen/dimen_24dp"
    android:background="@color/white">

    <TextView
        android:id="@+id/tvEquipmentModel"
        style="@style/Paragraph_400_bold_16_display_500"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toTopOf="parent"
        tools:text="Modelo de equipamento 1" />

    <include
        android:id="@+id/llEquipmentDetails"
        layout="@layout/layout_equipment_details_postecipado"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/dimen_8dp"
        app:layout_constraintTop_toBottomOf="@+id/tvEquipmentModel" />

</androidx.constraintlayout.widget.ConstraintLayout>